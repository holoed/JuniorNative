let item = mkParser (\inp -> if (nullStr inp) then []
                             else (headStr inp, tailStr inp) : [])
   
let or p q = mkParser(\inp -> let ret = runParser p inp in
                              if null ret then runParser q inp
                              else ret)
 
let many p = or ((>>=) p (\x ->
             (>>=) (many p) (\xs ->
             pure (x:xs)))) (pure [])
   
let main = runParser (many item) "Hello"    
