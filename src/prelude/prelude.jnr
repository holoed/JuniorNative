-- Junior Prelude 

val const :: a -> b -> a
let const x y = x

val id :: a -> a
let id x = x

val fst :: (a, b) -> a
let fst (x, y) = x

val snd :: (a, b) -> b
let snd (x, y) = y

val (.) :: (b -> c) -> (a -> b) -> a -> c
let (.) f g = \x -> f (g x)

val maybeToList :: Maybe a -> List a
let maybeToList = foldr (:) [] 

val listToMaybe :: List a -> Maybe a
let listToMaybe = foldr (const . Just) Nothing


